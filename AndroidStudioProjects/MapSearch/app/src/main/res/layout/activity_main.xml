<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity"
    android:background="@color/orange">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="50dp"
            android:layout_height="60dp"
            android:layout_marginStart="3dp"
            android:layout_marginLeft="3dp"
            android:layout_marginTop="3dp"
            android:layout_marginEnd="3dp"
            android:layout_marginRight="3dp"
            android:layout_marginBottom="3dp"
            android:background="@drawable/ic_home_foreground"
            android:tint="#000000"
            android:clickable="true"
            tools:ignore="VectorDrawableCompat" />
        <SearchView
            android:id="@+id/search"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/search"
            android:layout_margin="10dp"
            android:layout_marginLeft="150dp"
            />
    </LinearLayout>

    <com.google.android.gms.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="match_parent"
        android:layout_height="345dp"
        android:layout_marginTop="80dp"
        android:contentDescription="map" />

    <ImageButton
        android:id="@+id/GetLocation"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginLeft="355dp"
        android:layout_marginTop="365dp"
        android:background="@drawable/roundcorner"
        android:clickable="true"
        app:backgroundTint="#FFFFFF"
        app:srcCompat="@drawable/ic_mylocation_foreground"/>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="425dp"
        android:background="@drawable/bg">
        <Button
            android:id="@+id/button2"
            android:layout_width="350dp"
            android:layout_height="100dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="30dp"
            android:backgroundTint="@color/white"
            android:clickable="true" />
        <Button
            android:id="@+id/button3"
            android:layout_width="350dp"
            android:layout_height="110dp"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="150dp"
            android:layout_marginLeft="25dp"
            android:backgroundTint="@android:color/white"
            android:clickable="true" />
    </FrameLayout>

    <ImageView
        android:id="@+id/ClinicIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="445dp"
        app:srcCompat="@drawable/ic_clinic_foreground"
        tools:ignore="VectorDrawableCompat" />

    <TextView
        android:id="@+id/Clinic1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Clinic 1"
        android:layout_marginLeft="130dp"
        android:layout_marginTop="460dp"
        android:textColor="@color/black"
        android:textSize="16dp" />

    <ImageView
        android:id="@+id/Locationicon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="440dp"
        app:srcCompat="@drawable/ic_location_foreground"
        tools:ignore="VectorDrawableCompat" />

    <TextView
        android:id="@+id/Address1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Address"
        android:layout_marginLeft="160dp"
        android:layout_marginTop="480dp"
        android:textColor="@color/black"
        android:textSize="16dp" />

    <ImageView
        android:id="@+id/Favorite1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="270dp"
        android:layout_marginTop="440dp"
        app:srcCompat="@drawable/ic_favorite_foreground" />

    <ImageView
        android:id="@+id/Ratingicon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="260dp"
        android:layout_marginTop="480dp"
        app:srcCompat="@drawable/ic_star_foreground" />

    <TextView
        android:id="@+id/Rate1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="4.5/5"
        android:layout_marginLeft="330dp"
        android:layout_marginTop="523dp"
        android:textColor="@color/black"
        android:textSize="16dp" />

    <View
        android:id="@+id/divider2"
        android:layout_width="180dp"
        android:layout_height="5dp"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="510dp"
        android:background="?android:attr/listDivider" />

    <ImageView
        android:id="@+id/kmicon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="85dp"
        android:layout_marginTop="480dp"
        app:srcCompat="@drawable/ic_km_foreground" />

    <TextView
        android:id="@+id/km1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1.5km"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="523dp"
        android:textColor="@color/black"
        android:textSize="15dp" />

    <ImageView
        android:id="@+id/openicon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="170dp"
        android:layout_marginTop="480dp"
        app:srcCompat="@drawable/ic_km_foreground" />

    <TextView
        android:id="@+id/open1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="open"
        android:layout_marginLeft="240dp"
        android:layout_marginTop="523dp"
        android:textColor="@color/black"
        android:textSize="15dp" />

    <ImageView
        android:id="@+id/ClinicIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="575dp"
        app:srcCompat="@drawable/ic_clinic_foreground"
        tools:ignore="DuplicateIds,VectorDrawableCompat" />

    <TextView
        android:id="@+id/Clinic2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Clinic 2"
        android:layout_marginLeft="130dp"
        android:layout_marginTop="590dp"
        android:textColor="@color/black"
        android:textSize="16dp" />

    <ImageView
        android:id="@+id/Locationicon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="570dp"
        app:srcCompat="@drawable/ic_location_foreground"
        tools:ignore="DuplicateIds,VectorDrawableCompat" />

    <TextView
        android:id="@+id/Address2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Address"
        android:layout_marginLeft="160dp"
        android:layout_marginTop="610dp"
        android:textColor="@color/black"
        android:textSize="16dp" />

    <ImageView
        android:id="@+id/Favorite2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="270dp"
        android:layout_marginTop="570dp"
        app:srcCompat="@drawable/ic_favorite_foreground" />

    <ImageView
        android:id="@+id/Ratingicon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="260dp"
        android:layout_marginTop="610dp"
        app:srcCompat="@drawable/ic_star_foreground"
        tools:ignore="DuplicateIds" />

    <TextView
        android:id="@+id/Rate2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="4.0/5"
        android:layout_marginLeft="330dp"
        android:layout_marginTop="653dp"
        android:textColor="@color/black"
        android:textSize="16dp" />

    <View
        android:id="@+id/divider2"
        android:layout_width="180dp"
        android:layout_height="5dp"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="640dp"
        android:background="?android:attr/listDivider"
        tools:ignore="DuplicateIds" />

    <ImageView
        android:id="@+id/kmicon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="85dp"
        android:layout_marginTop="610dp"
        app:srcCompat="@drawable/ic_km_foreground"
        tools:ignore="DuplicateIds" />

    <TextView
        android:id="@+id/km2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1.8km"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="653dp"
        android:textColor="@color/black"
        android:textSize="15dp" />

    <ImageView
        android:id="@+id/openicon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="170dp"
        android:layout_marginTop="610dp"
        app:srcCompat="@drawable/ic_km_foreground"
        tools:ignore="DuplicateIds" />

    <TextView
        android:id="@+id/open2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="open"
        android:layout_marginLeft="240dp"
        android:layout_marginTop="653dp"
        android:textColor="@color/black"
        android:textSize="15dp" />

</RelativeLayout>